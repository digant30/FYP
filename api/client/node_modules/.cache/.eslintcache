();\n\n    fetch(\n      \"http://localhost:4000/channels/mychannel/transactions/\" + transactionId,\n      {\n        method: \"GET\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Authorization: \"Bearer \" + localStorage.getItem(\"token\"),\n        },\n      }\n    )\n      .then(function (response) {\n        return response.json();\n      })\n      .then(function (data) {\n        alert(\"Transaction retreival successful\");\n        console.log(data);\n\n        let container = document.getElementById(\"container\");\n\n        let table = document.createElement(\"table\");\n\n        data.forEach((item) => {\n          let tr = document.createElement(\"tr\");\n\n          let vals = Object.values(item);\n\n          vals.forEach((elem) => {\n            let td = document.createElement(\"td\");\n            td.innerText = elem;\n            tr.appendChild(td);\n          });\n          table.appendChild(tr);\n        });\n        container.appendChild(table);\n      })\n      .catch((error) => console.error(\"Error:\", error));\n  }\n\n  return (\n    <div>\n      <div class={styles.topnav}>\n        <a href=\"/\">Home</a>\n        <a href=\"register\">Register</a>\n        <a href=\"login\">Login</a>\n        <a href=\"addproduct\">Add Product</a>\n        <a href=\"addseller\">Add Seller</a>\n        <a href=\"changeowner\">Change Owner</a>\n        <a href=\"authenticate\">Authenticate</a>\n        <a href=\"queryall\">Query All Products</a>\n        <a href=\"queryprod\">Query Product</a>\n        <a href=\"addconsumer\">Add Consumer</a>\n        <a href=\"queryownerprod\">Query Product Owner</a>\n        <a href=\"queryseller\">Query Seller</a>\n        <a href=\"queryconsumer\">Query Consumer</a>\n        <a class={styles.active} href=\"querytransaction\">\n          Query Transaction\n        </a>\n        <a href=\"queryassethistory\">Query Asset</a>\n      </div>\n      <h1>Query Transaction</h1>\n      <form onSubmit={QueryTransaction}>\n        <input\n          value={transactionId}\n          onChange={(e) => settransactionId(e.target.value)}\n          type=\"text\"\n          placeholder=\"Tranasaction ID\"\n        />\n        <br />\n\n        <h4 id=\"prod\"></h4>\n        <input type=\"submit\" value=\"Query Transaction\" />\n        <thead>\n          <th>Product Name</th>\n          <th>Color</th>\n          <th>Brand</th>\n          <th>Mail</th>\n          <th>Consumer Name</th>\n          <th>Product ID</th>\n          <th>Seller ID</th>\n        </thead>\n        <container id=\"container\"></container>\n      </form>\n    </div>\n  );\n}\n\nexport default App;\n","/home/digant/BasicNetwork-2.0/api/client/src/pages/QueryAllSellers.js",["230","231"],"/home/digant/BasicNetwork-2.0/api/client/src/pages/QueryAllConsumers.js",["232","233"],"/home/digant/BasicNetwork-2.0/api/client/src/pages/QueryConsumerByContactNumber.js",["234"],"/home/digant/BasicNetwork-2.0/api/client/src/pages/QueryConsumerManu.js",["235"],"/home/digant/BasicNetwork-2.0/api/client/src/pages/QueryAllConsumersManu.js",["236","237"],"/home/digant/FYP/api/client/src/index.js",[],["238","239"],"/home/digant/FYP/api/client/src/App.js",[],"/home/digant/FYP/api/client/src/pages/AddProduct.js",[],"/home/digant/FYP/api/client/src/pages/RegisterUser.js",[],"/home/digant/FYP/api/client/src/pages/ChangeOwnership.js",[],"/home/digant/FYP/api/client/src/pages/AuthenticateProd.js",[],"/home/digant/FYP/api/client/src/pages/HomePage.js",[],"/home/digant/FYP/api/client/src/pages/QueryOwnerProducts.js",[],"/home/digant/FYP/api/client/src/pages/AddSeller.js",[],"/home/digant/FYP/api/client/src/pages/QueryAllProducts.js",[],"/home/digant/FYP/api/client/src/pages/QueryProduct.js",[],"/home/digant/FYP/api/client/src/pages/QueryAllConsumersManu.js",[],"/home/digant/FYP/api/client/src/pages/QueryConsumer.js",[],"/home/digant/FYP/api/client/src/pages/QuerySeller.js",[],"/home/digant/FYP/api/client/src/pages/QueryTransac